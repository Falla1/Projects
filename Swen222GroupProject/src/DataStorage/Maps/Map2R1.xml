	<Map>
		<!-- map layout
		b = baseLayerBlock
		x = normal block
		m = moveable block
		- = open door block
		r = change room block
		! = end block
		c = circle entrance
		s = square entrance
		p = triangle entrance
		@ = chest right
		# = chest left
		& = empty open chest
		* = closed chest
		^ = Pyramid ShapeItem
		o = Sphere ShapeItem
		+ = Square ShapeItem
		$ = Coin Game Item
		e = End -->
